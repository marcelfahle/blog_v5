{
  "name": "eleventy-high-performance-blog",
  "version": "5.0.2",
  "license": "MIT",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "build-ci": "pnpm js-build && eleventy && pnpm test && pnpm css",
    "build": "pnpm clean && pnpm build-ci",
    "watch": "concurrently \"pnpm serve\" \"pnpm js-build-watch\" \"pnpm test-watch\" \"pnpm css-watch\"",
    "serve": "pnpm clean && eleventy --serve",
    "js-build": "rollup -c rollup.config.js",
    "js-build-watch": "rollup -c rollup.config.js -w",
    "debug": "DEBUG=* eleventy && pnpm test",
    "test": "mocha test/test*.js",
    "test-watch": "mocha test/test*.js --watch --watch-files=test/*",
    "css-watch": "tailwindcss -i css/tailwind.css -o _site/css/tailwind.css --watch --postcss",
    "css": "tailwindcss -i css/tailwind.css -o _site/css/tailwind.css --postcss",
    "clean": "rm -Rf ./_site/posts/*",
    "dev:refresh": "pnpm build && pnpm serve"
  },
  "pre-push": [
    "build"
  ],
  "pre-commit": [],
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-navigation": "^0.1.3",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "autoprefixer": "^10.4.13",
    "eleventy-plugin-local-images": "^0.4.0",
    "file-type": "^16.5.4",
    "fs-extra": "^8.1.0",
    "jsdom": "^16.5.0",
    "luxon": "^2.5.2",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^5.2.5",
    "node-fetch": "^2.6.7",
    "postcss": "^8.4.21",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.4",
    "rollup-plugin-terser": "^6.1.0",
    "shorthash": "0.0.2",
    "tailwindcss": "^3.2.4"
  },
  "dependencies": {
    "@ampproject/toolbox-optimizer": "^2.9.0",
    "@portabletext/to-html": "^3.0.0",
    "@sanity/cli": "^4.10.3",
    "@sanity/client": "^7.12.0",
    "@sanity/vision": "^4.10.2",
    "any-shell-escape": "^0.1.1",
    "clean-css": "^4.2.3",
    "concurrently": "^7.0.0",
    "csp-hash-generator": "0.0.3",
    "csso": "^4.0.3",
    "datauri": "^3.0.0",
    "dotenv": "^17.2.3",
    "expect.js": "^0.3.1",
    "ffmpeg-static": "^4.4.0",
    "gray-matter": "^4.0.3",
    "hasha": "^5.2.0",
    "html-minifier": "^4.0.0",
    "image-size": "^0.8.3",
    "lru-cache": "^5.1.1",
    "mocha": "^10.1.0",
    "phin": "^3.5.0",
    "purge-from-html": "^1.0.3",
    "purgecss": "^4.0.3",
    "querystring": "^0.2.0",
    "rollup": "^2.23.0",
    "sanity": "^4.10.2",
    "sharp": "^0.34.4",
    "touch": "^3.1.0",
    "twitter": "^1.7.1"
  },
  "packageManager": "pnpm@9.12.3+sha1.4fb96925d434c491285182acf29803bb9aa2e6e5"
}
